    <script>
        function speech() {
            if (annyang) {
                var commands = {
                    'home': function() {
                        home();
                    },
                    'education': function() {
                        education();
                    },
                    'projects': function() {
                        projects();
                    },
                    'photography': function() {
                        photography();
                    },
                    'close': function() {
                        close_mod();
                    },
                    'validate': function() {
                        validate();
                    }
                }
            }
            annyang.debug();
            annyang.setLanguage('en-US');
            annyang.start({
                autoRestart: false,
                continuous: true
            });
        }
    </script>
    <script>
    function launch_modal(){
    
    	$('#address').modal('show').effect('shake');
    
    }
       function close_modal() {
            $('#address').modal('hide');
        }
   
    
    
    </script>
